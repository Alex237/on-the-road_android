<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Interdiction de tourner</string>
    <!-- go straight -->
    <string name="go_straight">Continuer</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Tourner légèrement à droite</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">À droite</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Tourner complètement à droite</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Demi-tour</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Tourner complètement à gauche</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">À gauche</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Tourner légèrement à gauche</string>
    <string name="reach_via_point">Atteindre via le point</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Avancer</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Entrer sur le rond-point</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Sortir du rond-point</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Rester sur le rond-point</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Commencer à la fin de la rue</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">Vous êtes arrivé</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Entrer</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Sortir</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s et continuer sur %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s pour %3$s</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Continuer sur %1$s pour %2$s</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Continuer sur %s</string>
</resources>
