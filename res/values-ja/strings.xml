<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">曲がれません</string>
    <!-- go straight -->
    <string name="go_straight">まっすぐに進む</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">次の通りを緩く右折</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">次の通りを右折</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">次の通りで右折する</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Uターン</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">次の通りで左折する</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">次の通りを左折</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">次の通りを緩く左折</string>
    <string name="reach_via_point">ポイントを経由して到着</string>
    <!-- drive on (streetname) -->
    <string name="head_on">次の通りを走っています</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">環状交差点へ入る</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">環状交差点を抜ける</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">環状交差点にとどまる</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">路地の端から開始する</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">到着しました</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">入力する</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">終了する</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s 、その後 %3$s、 %2$s へすすむ</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">この先%3$s 、 %1$s %2$s</string>
    <string name="full_instruction_destination">%1$s%2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">この先%2$s 、 %1$s へすすむ</string>
    <string name="simple_instruction">%1$s%2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">%s へすすむ</string>
</resources>
