<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Geen afslag</string>
    <!-- go straight -->
    <string name="go_straight">Ga door op</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Licht rechts aanhouden op</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">Rechts op</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Scherp naar rechts op</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">U-bocht</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Scherp naar links op</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">Links op</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Licht links aanhouden op</string>
    <string name="reach_via_point">Bereik via punt</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Ga door</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Benader rotonde</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Verlaat rotonde</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Blijf op rotonde</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Begin bij het einde van de straat</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">U bent gearriveerd</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Benader</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Verlaat</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s en ga door op voor %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s voor %3$s</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Ga door op %1$s voor %2$s</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Ga door op %s</string>
</resources>
