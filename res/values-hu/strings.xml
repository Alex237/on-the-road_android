<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Nincs kanyar</string>
    <!-- go straight -->
    <string name="go_straight">Folytatás</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Enyhén jobbra az utcán:</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">Jobbra az utcán:</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Élesen jobbra az utcán:</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Visszafordulás</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Élesen balra az utcán:</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">Balra az utcán:</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Enyhén balra az utcán:</string>
    <string name="reach_via_point">Köztes ponton át</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Előre az utcán:</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Hajtson be a körforgalomba</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Hajtson ki a körforgalomból</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Maradjon a körforgalomban</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Kezdés az utca végén</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">Megérkezett</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Belépés</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Kilépés</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s és menj tovább %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s %3$s kilométert</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Menjen tovább itt: %1$s, %2$s távolságot</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Tovább itt: %s</string>
</resources>
