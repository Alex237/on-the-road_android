<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Nessuna svolta</string>
    <!-- go straight -->
    <string name="go_straight">Continua dritto</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Leggermente a destra su</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">A destra su</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Svoltare completamente a destra</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Inversione a U</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Completamente a sinistra su</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">A sinistra su</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Leggermente a sinistra su</string>
    <string name="reach_via_point">Percorrere tramite</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Dritto su</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Entrare nella rotonda</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Lasciare la rotonda</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Restare presso la rotonda</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Iniziare alla fine della strada</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">Hai raggiunto la tua destinazione</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Entra</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Lascia</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s e continua per %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s per %3$s</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Continua su %1$s per %2$s</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Continua su %s</string>
</resources>
