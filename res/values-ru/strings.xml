<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Поворот запрещен</string>
    <!-- go straight -->
    <string name="go_straight">Продолжайте движение прямо</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Плавный поворот направо на</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">Поворот направо на</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Резкий поворот на</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Разворот</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Резкий поворот налево на</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">Поворот налево на</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Плавный поворот налево на</string>
    <string name="reach_via_point">Проехать через точку</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Курс на</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Въезд на участок с круговым движением</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Выезд с участка с круговым движением</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Оставайтесь на участке с круговым движением</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Начало в конце улицы</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">Вы прибыли в место назначения</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Войти</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Выйти</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s и продолжайте движение %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s %3$s</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Продолжайте движение по %1$s %2$s</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Продолжайте движение по %s</string>
</resources>
