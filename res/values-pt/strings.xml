<?xml version="1.0" encoding="UTF-8"?><resources>
    <!-- this set of text is displayed to the users while they are driving a car  with the app in navigation mode-->
    <!-- user cannot turn-->
    <string name="no_turn">Proibido virar</string>
    <!-- go straight -->
    <string name="go_straight">Siga em frente</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">Curva leve à direita na</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">À direita na</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">Curva acentuada à direita na</string>
    <!-- u turn/ turn around -->
    <string name="u_turn">Curva em U</string>
    <!-- sharp left turn on (streetname) -->
    <string name="turn_sharp_left">Curva acentuada à esquerda na</string>
    <!-- left turn on (streetname) -->
    <string name="turn_left">À esquerda na</string>
    <!-- slight left on (streetname) -->
    <string name="turn_slight_left">Curva leve à esquerda na</string>
    <string name="reach_via_point">Alcance via ponto</string>
    <!-- drive on (streetname) -->
    <string name="head_on">Siga pela</string>
    <!--  enter traffic circle -->
    <string name="enter_round_about">Entre na rotatória</string>
    <!-- leave traffic circle -->
    <string name="leave_round_about">Saia da rotatória</string>
    <!-- stay in traffic circle -->
    <string name="stay_on_round_about">Permaneça na rotatória</string>
    <!-- begin at the end of the street -->
    <string name="start_at_end_of_street">Comece no final da rua</string>
    <!-- you have reached your destiniation -->
    <string name="you_have_arrived">Você chegou</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">Entre</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">Saia</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s %2$s e continue emfrente por %3$s</string>
    <!-- will construct to a phrase like 'drive straight 2 miles' -->
    <string name="full_instruction_before_straight">%1$s %2$s por %3$s</string>
    <string name="full_instruction_destination">%1$s %2$s</string>
    <!-- will construct to a phrase like 'Continue on main street for 2 miles'-->
    <string name="full_instruction_after">Continue na %1$s por %2$s</string>
    <string name="simple_instruction">%1$s %2$s</string>
    <!-- will construct to a phrase like 'continue on main street' -->
    <string name="simple_instruction_after">Continue na %s</string>
</resources>
