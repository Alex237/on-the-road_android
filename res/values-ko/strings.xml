    <string name="go_straight">다음에서 계속 직진:</string>
    <!-- slight right turn on (streetname) -->
    <string name="turn_slight_right">다음에서 살짝 우회전:</string>
    <!-- right turn on (streetname) -->
    <string name="turn_right">다음에서 우회전:</string>
    <!-- sharp right turn on (streetname) -->
    <string name="turn_sharp_right">다음에서 급한 우회전:</string>
