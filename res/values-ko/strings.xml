    <string name="you_have_arrived">도착했습니다</string>
    <!-- enter -->
    <string name="enter_against_allowed_direction">진입</string>
    <!-- exit -->
    <string name="leave_against_allowed_direction">나가기</string>
    <!-- will construct to a phrase like 'turn left and continue on for 2 miles' -->
    <string name="full_instruction_before_default">%1$s%2$s 그리고 %3$s 정도 계속 주행</string>
